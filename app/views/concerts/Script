rows=["A","B","C","D","E","F","G","H","I","J","K","L","M","N","O","P","Q","R","S"];

(1..475).each do |i|
	s=Seat.new
	s.id=i
	s.concert_id= 44

	column= i % 25
	if column==0
		column=25
	end
	s.column=column

	rownum=i/25
	s.row=rows[rownum]

	if i>250
		s.price= 4500
	else
		s.price= 6000
	end

	s.save
end

