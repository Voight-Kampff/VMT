    <% Photo.select(:gallery).distinct.each do |f| %>
      <% @gallery=Photo.where('gallery' => f.gallery) %>
      <% @gallery.each_slice(6) do |row| %>
        <div class="row-fluid">
          <ul class="thumbnails">
          <% row.each do |pic| %>
            <li class="span2">
              <a class="thumbnail" rel="<%=@gallery.name%>" href="<%=pic.image.url%>" >
              <%= image.tag pic.image.url(:thumb) %>
            </li>
          <%end%>
          </ul>
        </div>
      <%end%>
    <%end%>

